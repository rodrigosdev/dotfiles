{
	"editor.minimap.enabled": false,
	"workbench.editor.enablePreview": false,
	"workbench.editor.enablePreviewFromQuickOpen": false,
	"editor.cursorStyle": "block",
	"editor.fontLigatures": false,
	"editor.fontSize": 18,
	"editor.fontFamily": "Geist Mono Light",
	"zenMode.fullScreen": false,
	"window.titleBarStyle": "custom",
	"javascript.validate.enable": false,
	"explorer.autoReveal": false,
	"editor.wordWrap": "on",
	"git.confirmSync": false,
	"keyboard.dispatch": "keyCode",
	"terminal.integrated.scrollback": 1000000000,
	"terminal.integrated.fontSize": 15,
	"typescript.preferences.importModuleSpecifier": "non-relative",
	"window.title": "${folderName} ${separator} ${activeEditorMedium} ${dirty} ${separator} ${appName}",
	"editor.formatOnPaste": false,
	"editor.formatOnSave": true,
	"editor.trimAutoWhitespace": false,
	"editor.renderWhitespace": "all",
	"editor.multiCursorModifier": "ctrlCmd",
	"workbench.editor.showTabs": "multiple",
	"editor.suggestSelection": "first",
	"breadcrumbs.enabled": false,
	"workbench.sideBar.location": "right",
	"security.workspace.trust.untrustedFiles": "open",
	"security.workspace.trust.banner": "never",
	"security.workspace.trust.enabled": false,
	"workbench.tree.indent": 15,
	"window.zoomLevel": -1,
	"git.openRepositoryInParentFolders": "never",
	"editor.inlineSuggest.suppressSuggestions": true,
	"editor.defaultFormatter": "esbenp.prettier-vscode",
	"[javascript]": {
		"editor.defaultFormatter": "biomejs.biome"
	},
	"[typescript]": {
		"editor.defaultFormatter": "biomejs.biome"
	},
	"[javascriptreact]": {
		"editor.defaultFormatter": "biomejs.biome"
	},
	"[typescriptreact]": {
		"editor.defaultFormatter": "biomejs.biome"
	},
	"[json]": {
		"editor.defaultFormatter": "biomejs.biome"
	},
	"[jsonc]": {
		"editor.defaultFormatter": "biomejs.biome"
	},
	"[css]": {
		"editor.defaultFormatter": "biomejs.biome"
	},
	"[graphql]": {
		"editor.defaultFormatter": "biomejs.biome"
	},
	"explorer.confirmDelete": false,
	"workbench.colorCustomizations": {
		"sideBar.background": "#282C34",
		"list.hoverBackground": "#555555",
		"focusBorder": "#00000000",
		"editorGroupHeader.tabsBackground": "#282C34",
		"tab.border": "#00000000",
		"tab.activeBorder": "#00000000",
		"tab.unfocusedActiveBorder": "#00000000",
		"tab.activeBorderTop": "#00000000",
		"scrollbar.shadow": "#00000000",
		"widget.shadow": "#00000000",
		"tab.inactiveBackground": "#282C34"
	},
	"explorer.confirmDragAndDrop": false,
	"explorer.sortOrder": "filesFirst",
	"window.newWindowDimensions": "maximized",
	"workbench.startupEditor": "none",
	"workbench.colorTheme": "Pastel Pro",
	"git.autofetch": true,
	"cursor.cpp.disabledLanguages": ["markdown"],
	"custom-ui-style.font.sansSerif": "Geist  Mono Medium"
}
