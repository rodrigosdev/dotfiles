{
  "editor.minimap.enabled": false,
  "workbench.editor.enablePreview": false,
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "editor.cursorStyle": "block",
  "editor.fontLigatures": false,
  "editor.fontSize": 18,
  "editor.fontFamily": "Geist Mono Light",
  "zenMode.fullScreen": false,
  "window.titleBarStyle": "custom",
  "javascript.validate.enable": false,
  "explorer.autoReveal": false,
  "editor.wordWrap": "on",
  "git.confirmSync": false,
  "keyboard.dispatch": "keyCode",
  "terminal.integrated.scrollback": 1000000000,
  "terminal.integrated.fontSize": 16,
  "terminal.integrated.fontFamily": "Geist Mono",
  "typescript.preferences.importModuleSpecifier": "non-relative",
  "window.title": "${folderName} ${separator} ${activeEditorMedium} ${dirty} ${separator} ${appName}",
  "editor.formatOnPaste": false,
  "editor.formatOnSave": true,
  "editor.trimAutoWhitespace": false,
  "editor.renderWhitespace": "all",
  "editor.multiCursorModifier": "ctrlCmd",
  "workbench.editor.showTabs": "multiple",
  "editor.suggestSelection": "first",
  "breadcrumbs.enabled": false,
  "workbench.sideBar.location": "right",
  "security.workspace.trust.untrustedFiles": "open",
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "left"
  },
  "security.workspace.trust.banner": "never",
  "security.workspace.trust.enabled": false,
  "extensions.ignoreRecommendations": true,
  "workbench.tree.indent": 15,
  "window.zoomLevel": -1,
  "git.openRepositoryInParentFolders": "never",
  "editor.inlineSuggest.suppressSuggestions": true,
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "explorer.confirmDelete": false,
  "workbench.colorCustomizations": {
    "sideBar.background": "#282C34",
    "list.hoverBackground": "#555555",
    "focusBorder": "#00000000",
    "editorGroupHeader.tabsBackground": "#282C34",
    "tab.border": "#00000000",
    "tab.activeBorder": "#00000000",
    "tab.unfocusedActiveBorder": "#00000000",
    "tab.activeBorderTop": "#00000000",
    "scrollbar.shadow": "#00000000",
    "widget.shadow": "#00000000",
    "tab.inactiveBackground": "#282C34"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "explorer.confirmDragAndDrop": false,
  "explorer.sortOrder": "filesFirst",
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "window.newWindowDimensions": "maximized",
  "workbench.startupEditor": "none",
  "workbench.colorTheme": "Pastel Pro",
  "git.autofetch": true,
  "custom-ui-style.font.sansSerif": "Geist Mono Medium",
  "workbench.layoutControl.enabled": false,
  "workbench.activityBar.location": "top",
  "workbench.editor.editorActionsLocation": "hidden",
  "window.commandCenter": false
}
